{
  "name": "custom-jeans",
  "version": "0.0.2",
  "description": "manages custom code for geenee templates",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "lintfix": "eslint \"src/**/*.ts\" --fix",
    "test": "eslint \"src/**/*.ts\"",
    "posttest": "eslint \"src/**/*.ts\"",
    "prepack": "rm -rf lib && tsc -b",
    "testreal": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "keywords": [
    "ts-packrat"
  ],
  "author": "YizYah",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.14.19",
    "chalk": "^4.1.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.2",
    "magicalstrings": "0.0.11",
    "path": "^0.12.7",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "mocha": "^7.1.2",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1"
  },
  "bugs": "https://github.com/YizYah/custom-jeans/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/YizYah/custom-jeans",
  "types": "lib/index.d.ts",
  "repository": "YizYah/custom-jeans"
}
